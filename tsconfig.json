{
  "compilerOptions": {
    "types": ["whatwg-url"],
    // "incremental": true,  /* Save .tsbuildinfo files to allow for incremental compilation of projects. */
    "target": "es2016", 
    "module": "CommonJS",
    "outDir": "dist",
    // "rootDir": "./",                                 
    // "baseUrl": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", "dist"]
}
